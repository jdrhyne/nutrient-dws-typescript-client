{
  "name": "nutrient-dws-typescript-client",
  "version": "0.1.4",
  "description": "Node.js TypeScript client library for Nutrient Document Web Services (DWS) API",
  "keywords": [
    "nutrient",
    "dws",
    "document",
    "pdf",
    "api",
    "client",
    "typescript",
    "nodejs"
  ],
  "homepage": "https://github.com/jdrhyne/nutrient-dws-typescript-client#readme",
  "bugs": {
    "url": "https://github.com/jdrhyne/nutrient-dws-typescript-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdrhyne/nutrient-dws-typescript-client.git"
  },
  "license": "MIT",
  "author": "Nutrient Team",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --project tsconfig.types.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "run:example": "tsx examples/workflow-builder.ts",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=\"^((?!integration|e2e).)*$\"",
    "test:integration": "jest --testPathPatterns=integration.test",
    "test:e2e": "jest --testPathPatterns=\"(integration|e2e-advanced).test\"",
    "test:integration:verbose": "jest --testPathPatterns=integration.test --verbose",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "generate:types": "openapi-typescript dws-api-spec.yml -o src/generated/api-types.ts"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "form-data": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "dotenv": "^17.0.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.2.0",
    "jest": "^30.0.3",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "tests"
  }
}
